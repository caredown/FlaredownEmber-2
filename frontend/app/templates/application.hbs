{{#application-wrapper as |config|}}
  {{#if config.isEmptySubdomain}}
    <header class="emptyHeader"></header>
  {{else}}
    {{partial "application/header"}}
  {{/if}}

  {{outlet}}

  {{#if config.isEmptySubdomain}}
    <footer></footer>
  {{else}}
    {{partial "application/footer"}}
  {{/if}}

  {{#if (and session.isAuthenticated (not config.isEmptySubdomain))}}
    {{bottom-nav}}
  {{/if}}

  {{outlet "modal"}}
{{/application-wrapper}}
